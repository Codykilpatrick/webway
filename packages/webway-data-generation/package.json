{
  "name": "@webway/data-generation",
  "version": "1.0.0",
  "description": "High-performance data generation utilities built with Rust",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "cargo build --release",
    "dev": "cargo run",
    "test": "cargo test",
    "lint": "cargo clippy -- -D warnings",
    "format": "cargo fmt",
    "format:check": "cargo fmt -- --check",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "clean": "cargo clean && rm -rf dist"
  },
  "devDependencies": {
    "@types/node": "20.0.0",
    "prettier": "3.1.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "data-generation",
    "rust",
    "performance",
    "utilities",
    "webway"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/webway.git",
    "directory": "packages/webway-data-generation"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@3.6.4"
}
